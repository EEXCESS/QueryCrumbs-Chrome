{
    "name": "QueryCrumbs",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at":"document_end",
            "js": ["js/lib/requirejs/require.js","js/requireContent.js","js/common.js","js/content.js"]
        }
    ],

    "web_accessible_resources": [
        "js/*",
        "media/*"
    ],
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "icons": {"16": "media/icons/16.png",
        "48": "media/icons/48.png",
        "128": "media/icons/128.png"}
}

